<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/lab4/Schemax/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xsd:import namespace="http://www.tibco.com/schemas/lab4/Schemax/Schema.xsd" schemaLocation="/Schemax/loggingSchema.xsd"/>
    <pd:name>ProcessDefinitions/Logging.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:logInfo"/>
    <pd:startX>84</pd:startX>
    <pd:startY>155</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>492</pd:endX>
    <pd:endY>156</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1618308108654</pd:targetNamespace>
    <pd:activity name="Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>290</pd:x>
        <pd:y>155</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>insert into commonlog (id_log,idtech,idfonc,status,logmessage,date_time) values(?,?,?,?,?,?)</statement>
            <jdbcSharedConfig>/Connections/Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>id_log</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>idtech</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>idfonc</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>status</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>logmessage</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>date_time</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <id_log>
                    <xsl:value-of select="tib:timestamp()"/>
                </id_log>
                <idtech>
                    <xsl:value-of select="$Start/pfx:logInfo/pfx:idTech"/>
                </idtech>
                <idfonc>
                    <xsl:value-of select="$Start/pfx:logInfo/pfx:idFonc"/>
                </idfonc>
                <status>
                    <xsl:value-of select="$Start/pfx:logInfo/pfx:status"/>
                </status>
                <logmessage>
                    <xsl:value-of select="$Start/pfx:logInfo/pfx:logMsg"/>
                </logmessage>
                <date_time>
                    <xsl:value-of select="current-dateTime()"/>
                </date_time>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Update</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Update</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>